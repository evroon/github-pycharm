{
    "manifest_version": 2,
    "name": "GitHub-PyCharm",
    "version": "1.1",
    "description": "Opens GitHub files in PyCharm.",
    "homepage_url": "https://github.com/evroon/github-pycharm",
    "developer": {
        "name": "evroon",
        "url": "https://github.com/evroon"
    },
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://github.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "background": {
        "scripts": ["background-script.js"]
    }
}
